{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true
    }
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.diagnostics.disable": [
        "missing-fields",
        "inject-field",
        "param-type-mismatch",
        "assign-type-mismatch"
      ]
    }
  }
}
